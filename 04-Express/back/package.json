{
  "name": "04-express",
  "version": "1.0.0",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "start": "run-p -l type-check:watch start:dev start:local-db",
    "start:dev": "tsx --watch src/index.ts",
    "start:console-runners": "tsx --watch src/console-runners/index.ts",
    "start:local-db": "docker compose up -d",
    "type-check": "tsc --noEmit --preserveWatchOutput",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "imports": {
    "#common/*": "./src/common/*",
    "#common-app/*": "./src/common-app/*",
    "#core/*": "./src/core/*",
    "#dals/*": "./src/common/*",
    "#pods/*": "./src/common/*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/inquirer": "^9.0.7",
    "inquirer": "^12.1.0",
    "npm-run-all": "^4.1.5",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
